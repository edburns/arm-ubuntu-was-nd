{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ibmUserId": {
            "value": "${ibmUserId}"
        },
        "ibmUserPwd": {
            "value": "${ibmUserPwd}"
        },
        "adminUser": {
            "value": "${adminUser}"
        },
        "adminPwd": {
            "value": "${adminPwd}"
        },
        "vmAdminId": {
            "value": "${vmAdminId}"
        },
        "vmAdminPwd": {
            "value": "${vmAdminPwd}"
        },
        "dnsLabelPrefix":{
            "value": "${dnsLabelPrefix}"
        },
        "virtualMachineName": {
            "value": "${virtualMachineName}"
        },
        "virtualNetworkName": {
            "value": "${virtualNetworkName}"
        },
        "addressPrefix": {
            "value": "${addressPrefix}"
        },
        "subnetName": {
            "value": "${subnetName}"
        },
        "subnetAddressPrefix": {
            "value": "${subnetAddressPrefix}"
        },
        "db2ServerName": {
            "value": "${db2ServerName}"
        },
        "db2ServerPortNumber": {
            "value": "${db2ServerPortNumber}"
        },
        "db2DBName": {
            "value": "${db2DBName}"
        },
        "db2DBUserName": {
            "value": "${db2DBUserName}"
        },
        "db2DBUserPwd": {
            "value": "${db2DBUserPwd}"
        },
        "connectToDB2Server": {
            "value": ${connectToDB2Server}
        }
    }
}